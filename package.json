{
  "name": "gravatar-squid",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "db:migrate": "npx squid-typeorm-migration apply",
    "squid-etl:start": "node lib/processor.js",
    "squid-api:start": "squid-graphql-server"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/big-decimal": "^1.0.0",
    "@subsquid/evm-processor": "^1.8.5",
    "@subsquid/graphql-server": "4.5.0",
    "@subsquid/typeorm-migration": "1.3.0",
    "@subsquid/typeorm-store": "1.2.6",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "3.2.4",
    "@subsquid/typeorm-codegen": "1.3.3",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "typechain": "^8.3.2",
    "typescript": "~5.2.2"
  }
}
